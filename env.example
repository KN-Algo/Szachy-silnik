# Konfiguracja połączenia z brokerem MQTT

# Host i port brokera (np. lokalny Mosquitto)
MQTT_HOST=localhost
MQTT_PORT=1883

# Identyfikator klienta MQTT (powinien być unikalny w ramach brokera)
MQTT_CLIENT_ID=chess-engine

# Dane logowania do brokera (opcjonalnie, zostaw puste jeśli nie ma autoryzacji)
MQTT_USERNAME=
MQTT_PASSWORD=

# Prefiks dla topiców (obecnie nieużywany w kodzie, można zostawić pusty)
MQTT_PREFIX=
